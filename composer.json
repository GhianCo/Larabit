{
  "name": "ghianco/larabit",
  "description": "Larabit Code Generator.",
  "license": "MIT",
  "authors": [
    {
      "name": "GhianCo Zapata",
      "email": "ghiancorx@gmail.com",
      "homepage": "https://github.com/GhianCo"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "ext-pdo": "*",
    "ext-json": "*",
    "pimple/pimple": "^3.2",
    "slim/psr7": "^1.4.0",
    "slim/slim": "^4.5",
    "vlucas/phpdotenv": "^5.1"
  },
  "require-dev": {
    "symfony/console": "^3.3"
  },
  "config": {
    "sort-packages": true,
    "process-timeout": 0
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/GhianCo/larabit"
    }
  ]
}
